                <NavigationViewItem Content="DemoList" x:Name="demoList" />
                <NavigationViewItem Content="EmptyPage" x:Name="emptyPage" IsSelected="True"  />
            </NavigationView.MenuItems>
            <Frame Margin="0,0,0,0" x:Name="contentFrame5" />
        </NavigationView>
    </Grid>
</Window>
